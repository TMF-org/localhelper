{
  "kind": "collectionType",
  "collectionName": "services",
  "info": {
    "singularName": "service",
    "pluralName": "services",
    "displayName": "Service"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "summary": {
      "type": "richtext"
    },
    "description": {
      "type": "richtext"
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false
    },
    "isPopular": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "isBookable": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "url": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::service-category.service-category",
      "inversedBy": "services"
    }
  }
}
