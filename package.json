{
  "name": "localhelper",
  "version": "1.0.0",
  "repository": "git@github.com:TMF-org/localhelper.git",
  "license": "GPL-3.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "start:frontend": "turbo run start --filter=frontend",
    "start:strapi": "turbo run start --filter=strapi",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "lint": "turbo run lint && prettier --check \"**/*.{ts,tsx,md,json}\"",
    "clean": "turbo run clean"
  },
  "devDependencies": {
    "prettier": "~2.8.4",
    "turbo": "~1.8.3",
    "typescript": "~5.0.2"
  },
  "resolutions": {
    "typescript": "~5.0.2"
  }
}
